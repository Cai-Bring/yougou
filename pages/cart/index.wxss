/* pages/cart/index.wxss */
.content{
  border-bottom: 60rpx solid #f5f5f5;
  width: 750rpx;
}
.contact{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 35rpx 30rpx;
}
.phone{
  display: flex;
  align-items: center;
}
.address{
  padding: 0 30rpx;
  padding-bottom: 30rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.content image{
  width: 100%;
  height: 15rpx;
}